<div class="container">
  <h2>All Workers You Manage</h2>           
  <table class="table table-striped">
    <tbody>
    <%= for worker <- @workers do %>
      <tr>
        <td><%= link(worker.worker_name, to: Routes.sheet_path(@conn, :manager_sheet_list, worker_id: worker.worker_id)) %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
  <%= link("Back", class: "btn btn-outline-primary btn-block", to: Routes.page_path(@conn, :manager_page)) %>
</div>